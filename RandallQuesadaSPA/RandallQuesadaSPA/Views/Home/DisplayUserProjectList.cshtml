@using RandallQuesadaSPA.ViewModel
@using RandallQuesadaSPA.Interfaces

@model SpaModel
@{ 
    AjaxOptions o = new AjaxOptions();
    o.OnSuccess = "OnGetListSuccess";

}
<table id="tblProduct" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th class="th-sm">
                <a href="">Project Id</a>
                <i class="fa fa-sort float-right" aria-hidden="true"></i>
            </th>
            <th class="th-sm">
                <a href="">Start Date</a>
                <i class="fa fa-sort float-right" aria-hidden="true"></i>
            </th>
            <th class="th-sm">
                <a href="">Time to Start</a>
                <i class="fa fa-sort float-right" aria-hidden="true"></i>
            </th>
            <th class="th-sm">
                <a href="">End Date</a>
                <i class="fa fa-sort float-right" aria-hidden="true"></i>
            </th>
            <th class="th-sm">
                <a href="">Credits</a>
                <i class="fa fa-sort float-right" aria-hidden="true"></i>
            </th>
            <th class="th-sm">
                <a href="">Status</a>
                <i class="fa fa-sort float-right" aria-hidden="true"></i>
            </th>
        </tr>
    </thead>
    <tbody>
               @foreach (UserProjectViewBase item in Model.UserBaseProjectList)
               {
                <tr>
                    <td>@item.ProjectId</td>
                    <td>@item.StartDate</td>
                    <td>@item.TimeToStart</td>
                    <td>@item.EndDate</td>
                    <td>@item.Credits</td>
                    <td>@item.Status</td>
                </tr>
               }                
</tbody>
</table>